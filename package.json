{
  "name": "openwithkraken",
  "displayName": "Open with GitKraken",
  "description": "Open current project folder / workspace with GitKraken",
  "version": "1.0.0",
  "publisher": "s3anmorrow",
  "engines": {
    "vscode": "^1.77.0"
  },
  "repository": {
		"type": "git",
		"url": "https://github.com/s3anmorrow/VSCodeExt-openWithKraken.git"
	},
  "license": "GPL-3.0",
	"categories": [
		"Other"
	],
	"keywords": [
		"gitkraken",
		"git",
		"versioning",
		"open",
		"kraken",
		"openwithkraken",
		"extension",
		"development",
		"UI",
		"vscode"
	],
	"icon": "images/icon.png",
  "activationEvents": [ "onStartupFinished" ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "openwithkraken.open",
        "title": "Open folder with GitKraken"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.77.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "eslint": "^8.36.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5",
    "@vscode/test-electron": "^2.3.0"
  }
}
